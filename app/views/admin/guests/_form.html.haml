= simple_form_for [:admin, @guest] do |f|
  = f.input :last_name
  = f.input :first_name
  = f.input :date_of_birth, as: :date, start_year: Date.today.year - 90, end_year: Date.today.year, order: [:day, :month, :year]
  = f.input :birthplace
  = f.input :telephone_number
  = f.input :email
  = f.submit
